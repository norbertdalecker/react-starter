<!doctype html>
<html>
<head>
  <meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title><%= htmlWebpackPlugin.options.title %></title>

  <link rel="manifest" href="<%= htmlWebpackPlugin.options.publicPath %>manifest.json">

	<% for (const key in htmlWebpackPlugin.files.css) { %>
		<link rel="stylesheet" href="<%= htmlWebpackPlugin.files.css[key] %>">
	<% } %>
</head>
<body>
  <main id="root"></main>

  <script>
    if ('serviceWorker' in navigator) {
      window.addEventListener('load', () => {
        navigator.serviceWorker.register('/sw.js');
      });
    }
  </script>

  <% for (const key in htmlWebpackPlugin.files.js) { %>
	  <script src="<%= htmlWebpackPlugin.files.js[key] %>"></script>
	<% } %>
</body>
</html>
